a = int(input())
b = int(input())
c = int(input())
if a < b < c:
    print(c - a)
if a > b > c:
    print(a - c)
if a > c > b:
    print(a - b)
if a < c < b:
    print(b - a)
if b > a > c:
    print(b - c)
if b < a < c:
    print(c - b)

a = int(input())
b = int(input())
print(a + b)

n = int(input())
hop = 0
for i in range(0, n + 1, 1):
    hop += i
print(hop)
